<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zfr.ctfzoneclient">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <service
            android:name=".service.view.TestService"
            android:exported="true"></service>
        <service
            android:name=".service.view.OrderService"
            android:exported="true" />
        <service
            android:name=".service.view.AuthService"
            android:exported="true" />

        <receiver
            android:name=".receivers.receivers.UserBroadcastReceiver"
            android:enabled="true"
            android:exported="true">
            <intent-filter>
                <action android:name="com.zfr.ctfzoneclient.MANAGE_USER" />
            </intent-filter>
        </receiver>

        <service
            android:name=".service.view.UserService"
            android:enabled="true"
            android:exported="true" />

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service
            android:name=".service.LaunchService"
            android:enabled="true"
            android:exported="true" />
    </application>

</manifest>